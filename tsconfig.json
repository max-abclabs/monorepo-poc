{
  "compilerOptions": {
    "target": "esnext",
    "useDefineForClassFields": true,
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "lib": [
      "esnext",
      "dom"
    ],
    "baseUrl": "./",
    "paths": {
      "@careos/components": [
        "libs/components"
      ],
      "@careos/components/*": [
        "libs/components/*"
      ],
      "@careos/frida": [
        "libs/frida"
      ],
      "@careos/frida/*": [
        "libs/frida/*"
      ],
      "@careos/test": [
        "libs/test"
      ],
      "@careos/test/*": [
        "libs/test/*"
      ],
      "@careos/testy": [
        "libs/testy"
      ],
      "@careos/testy/*": [
        "libs/testy/*"
      ],
      "@careos/Yeah": [
        "libs/Yeah"
      ],
      "@careos/Yeah/*": [
        "libs/Yeah/*"
      ],
      "@careos/shared": [
        "libs/shared"
      ],
      "@careos/shared/*": [
        "libs/shared/*"
      ]
    }
  },
  "include": [
    "apps/**/*.ts",
    "apps/**/*.d.ts",
    "apps/**/*.tsx",
    "apps/**/*.vue",
    "libs/**/*.ts",
    "libs/**/*.d.ts",
    "libs/**/*.tsx",
    "libs/**/*.vue"
  ]
}